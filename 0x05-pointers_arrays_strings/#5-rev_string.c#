#include "main.h"
/**
 * rev_string - a function to reverse a string
 * @s:pointer pointing to the string characters
 * Return: nothing
 */

void rev_string(char *s)
{
int i = 0;
int j;
char k;

while (*(s + i) != '\0')
{
  i += 1;

i -= 1;

while (j < len)
{
k = s[len];
s[len] = s[j];
s[j] = k;

j++;
len--;
}
}
