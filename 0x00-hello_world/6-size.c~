#include<stdio.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
  printf("Size of a char:%lu"sizeof(char));
  printf("Size of a char:%lu"sizeof(int));
  printf("Size of a char:%lu"sizeof(long int));
  printf("Size of a char:%lu"sizeof(long long int));
  printf("Size of a char:%lu"sizeof(long long int));
return (0);
}
