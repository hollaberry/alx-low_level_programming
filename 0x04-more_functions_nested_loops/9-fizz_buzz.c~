#include <stdio.h>
/**
 * main - Program to print Fizz, Buzz and FizzBuzz when i = 3, 5 and *both respectively
 * Return: Always 0 (success)
 */

int main(void)
{
int i;

for (i = 1; i <= 100; i++)
{
if ((i % 3 == 0) && !(i % 5 == 0) && !(i % 15 == 0))
{
printf("Fizz");
}
else if ((i % 5 == 0) && !(i % 15 == 0))
{
printf("Buzz");
}
else if (i % 15 == 0)
{
printf("FizzBuzz");
}
else if (i == 1)
{
printf("%d", i);
}
else
{
printf("%d", i);
}
printf(" ");
}
printf("\n");
return (0);
}
